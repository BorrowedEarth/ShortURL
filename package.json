{
  "name": "eled-short-url-service",
  "version": "1.0.0",
  "description": "Professional URL shortening service with domain masking for eled.org",
  "main": "index.html",
  "scripts": {
    "prestart": "node scripts/generate-seed.js",
    "start": "node server.js",
    "start:static": "serve -s -l 8000 .",
    "start:py": "python -m http.server 8000",
    "start:php": "php -S localhost:8000",
    "build": "node scripts/generate-seed.js",
    "build:force": "node scripts/generate-seed.js --force",
    "start:force": "node scripts/generate-seed.js --force && serve -s -l 8000 .",
    "test": "echo \"No tests specified\""
  },
  "keywords": [
    "url-shortener",
    "redirect",
    "web-service",
    "domain-masking",
    "eled",
    "education"
  ],
  "author": "eled.org Development Team",
  "license": "Proprietary",
  "repository": {
    "type": "git",
    "url": "https://github.com/eled-org/short-url-service.git"
  },
  "bugs": {
    "url": "https://github.com/eled-org/short-url-service/issues"
  },
  "homepage": "https://eled.org",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.19.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "serve": "^14.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
